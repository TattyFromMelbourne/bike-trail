{
  "parser": "babel-eslint",
  "settings": {
    "react": {
      "pragma": "React",
      "version": "^16.8.6",
    },
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
      "browser": true,
      "es6": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": 0,
    "prefer-const": 2
  },
  "globals": {
    "ReactElement": false
  }
}
